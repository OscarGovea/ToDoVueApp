itle: String,\r\n        description: String,\r\n        date: String\r\n    },\r\n    data: () => ({\r\n        show:'',\r\n    }),\r\n    methods: {\r\n        openNewNote() {\r\n            alert(\"ok\");\r\n        },\r\n    },\r\n};\r\n</script>\r\n  \r\n<style>\r\n.myCardNote {\r\n    max-height: 230px;\r\n    /* From https://css.glass */\r\n    background: rgba(255, 255, 255, 0.3) !important;\r\n    border-radius: 16px;\r\n    box-shadow: 0 4px 30px rgb(0 0 0 / 10%) !important;\r\n    backdrop-filter: blur(5.3px) !important;\r\n    -webkit-backdrop-filter: blur(5.3px) ;\r\n    border: 1px solid rgba(255, 255, 255, 0.57) !important;\r\n}\r\n\r\n.myCardText {\r\n    max-height: 90px;\r\n    overflow: hidden;\r\n}\r\n\r\n.textOverfloeEllipsis {\r\n    text-overflow: ellipsis;\r\n    text-align: justify;\r\n}\r\n.myCardActions{\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    margin: 10px 5px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}